<div class="col-md-8">
  <%= form_for @registered_applications do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control', placeholder: "Enter app name." %>
      <%= f.label :url %>
      <%= f.text_field :url, class: 'form-control', placeholder: "Enter app url." %>
    </div>
    <div class="form-group">
      <%= f.submit "Save", class: 'btn btn-success' %>
  <% end %>
</div>